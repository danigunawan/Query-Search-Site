<% if @error.blank? %>
  <% if @partial_results.present? %>
    <% if @graph_data.present? %>
      $('#results').empty().append("<%= escape_javascript(render partial: 'home/results_graph', locals: { data: @graph_data } )%>");
    <% end %>
    $('#results').append("<%= escape_javascript(render partial: 'home/results', locals: { tweets: @partial_results, next_page: @next_page, query: params[:search], count: @total_results, from_date: params[:from], to_date: params[:to] } ) %>");
  <% else %>
    $('#results').empty().html("No Results Found");
  <% end %>
<% else %>
  $('#results').empty().html("<%= @error %>");
<% end %>
<% if params[:search].present? %>
<% if false %>
$('#results').append("<%= escape_javascript(render partial: 'home/engine_results', locals: { query: params[:search], google_results: @google_results, google_page: @google_page }) %>")
<% end %>
<% end %>

$('#timer-container').empty();
$('.loader').addClass('hidden');
