<% if @error.blank? %>
  <% if @partial_results.present? %>
    <% if @graph_data.present? %>
      $('#results').empty().append("<%= escape_javascript(render partial: 'home/results_graph', locals: { data: @graph_data } )%>");
    <% end %>
    $('#results').append("<%= escape_javascript(render partial: 'home/results', locals: { tweets: @partial_results, query: params[:search], count: @total_results, max_id: @max_id, prev_id: @prev_id, from_date: params[:from], to_date: params[:to], show_zero: params[:show_zero] } ) %>");
  <% else %>
    $('#results').empty().html("No Results Found");
  <% end %>
<% else %>
  $('#results').empty().html("<%= @error %>");
<% end %>
<% if params[:search].present? %>
<% end %>

$('.loader').addClass('hidden');
